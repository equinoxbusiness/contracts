contract subscriptionMain is Context, ReentrancyGuard {
    
    using SafeMath for uint256;
    
    IERC20 public _token;
    address public _multisig;
    address public _wallet;
    address public _contractOwner;
    uint256 public _weiRaised;
    uint256 public _rate;
    uint256 public _startTime;
    uint256 public _endTime;
    uint256 public _minBuy;
    uint256 public _maxBuy;
    uint256 public _softCap;
    uint256 public _hardCap;
    uint256 public _soldTokens;
    
    IUniswapV2Router02 public router;

    address public pair;
    uint256 public bnbLockTime;
    uint256 public liquidityLockTime;
    uint256 public fundsLockTime;
    bool public presaleClosed;

    mapping(address => uint256) public _contributions;

    modifier onlyOwner(){
        require(_contractOwner == _msgSender(), "Function called from non admin wallet.");
        _;
    }

    modifier ifPresaleOpen(){
        require(block.timestamp >= _startTime, "");
        require(block.timestamp <= _endTime, "");
        _;
    }

    modifier ifPresaleEnded(){
        require(block.timestamp >= _endTime, "Presale is still open");
        _;
    }

    modifier ifPresaleNotClosed(){
        require(!presaleClosed, "Presale already closed");
        _;
    }

    modifier ifSoftcapReached(){
        require(_weiRaised >= _softCap, "Soft cap not reached");
        _;
    }

    modifier ifSoftcapNotReached(){
        require(_weiRaised < _softCap, "Soft cap reached");
        _;
    }

    event TokensPurchased(address indexed wallet, address indexed account, uint256 amount, uint256 tokenAmount);
    event Claimed(address indexed wallet, uint256 amount);
    event Deposited(address indexed wallet, uint256 amount);
    event LiquidityAdded(address owner, uint256 amountToken, uint256 bnbAmount);
    event LiquidityRemoved(address owner);
    
    constructor (address token_, address multisig_, address productOwner_, uint256 startTime_, uint256 endTime_, uint256 rate_, uint256 softCap_, uint256 hardCap_, address router_) {
        require(address(token_) != address(0), "Crowdsale: token is the zero address");
        _token = IERC20(token_);
        _startTime = startTime_;
        _endTime = endTime_;
        _wallet = productOwner_;
        _contractOwner = _msgSender();
        _multisig = multisig_;
        _softCap = softCap_ *1*10**18;
        _hardCap = hardCap_ *1*10**18;
        _rate = rate_;

        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(router_);

        IUniswapV2Factory factory = IUniswapV2Factory(_uniswapV2Router.factory());
        
        pair = factory.getPair(address(_token), _uniswapV2Router.WETH());

        if(pair == address(0)){
            pair = IUniswapV2Factory(_uniswapV2Router.factory())
                .createPair(address(_token), _uniswapV2Router.WETH());
        }
        router = _uniswapV2Router;

    }

    receive() external payable {
        buyTokens(_msgSender());
    }

    function token() public view returns (IERC20) {
        return _token;
    }

    
    function wallet() public view returns (address) {
        return _wallet;
    }

    function contractOwner() public view returns (address) {
        return _contractOwner;
    }
 
    function rate() public view returns (uint256) {
        return _rate;
    }
    
    function multisigAddress() public view returns(address){
        return _multisig;
    }

    function weiRaised() public view returns (uint256) {
        return _weiRaised;
    }

    function buyTokens(address beneficiary) public nonReentrant ifPresaleOpen payable {
        require(block.timestamp >= _startTime, "Presale has not started yet.");
        require(block.timestamp <= _endTime, "Presale has already ended.");
        uint256 weiAmount = msg.value;
        uint256 taxAmount = weiAmount.mul(32).div(1000);
        uint256 contributionAmount = weiAmount.sub(taxAmount);
        require(weiAmount > 0, "Amount should be greater then 0.");
        _preValidatePurchase(beneficiary, contributionAmount);
        _ifEligible(beneficiary, contributionAmount);

        _contributions[beneficiary] += contributionAmount;
        payable(_wallet).transfer(taxAmount);
        emit Deposited(beneficiary, contributionAmount);
        _weiRaised += contributionAmount;
        _soldTokens = _soldTokens.add(_getTokenAmount(contributionAmount));
    }

    function _preValidatePurchase(address beneficiary, uint256 weiAmount) internal view {
        require(beneficiary != address(0), "Crowdsale: beneficiary is the zero address");
        require(weiAmount != 0, "Crowdsale: weiAmount is 0");
        this;
    }

    function _postValidatePurchase(address beneficiary, uint256 weiAmount) internal view {
        // solhint-disable-previous-line no-empty-blocks
    }

    function _deliverTokens(address beneficiary, uint256 tokenAmount) internal {
        _token.transfer(beneficiary, tokenAmount);
    }

    function _processPurchase(address beneficiary, uint256 tokenAmount) internal {
        _contributions[beneficiary] = 0;
        _deliverTokens(beneficiary, tokenAmount);
    }

    function _updatePurchasingState(address beneficiary, uint256 weiAmount) internal {
        // solhint-disable-previous-line no-empty-blocks
    }

    function _getTokenAmount(uint256 weiAmount) internal view returns (uint256) {
        return weiAmount.mul(_rate);
    }

    function _forwardFunds(uint256 amount) internal {
        payable(_wallet).transfer(amount);
    }
    
    function end() public ifPresaleEnded ifSoftcapReached onlyOwner {

        uint256 unsoldTokens = _token.balanceOf(address(this)).sub(_soldTokens);
        _token.transfer(_multisig, unsoldTokens);
        uint256 contractBalance = address(this).balance;
        payable(_multisig).transfer(contractBalance);
        //fundsLockTime = block.timestamp + 0 days;
        presaleClosed = true;
        
    }

    function withdrawLockedFundsIfICOFailed() public ifPresaleEnded ifSoftcapNotReached onlyOwner {
        _token.transfer(_multisig, _token.balanceOf(address(this)));
    }

    function withdrawLockedFunds() public ifPresaleEnded onlyOwner {
        require(presaleClosed, "Presale not closed yet");
        require(fundsLockTime < block.timestamp, "Still locked.");
        _token.transfer(_multisig, _token.balanceOf(address(this)));
    }

    function withdrawETH() public {
        uint256 weiContributed = _contributions[_msgSender()];
        require(weiContributed > 0, "Wallet have no contribution.");
        _contributions[_msgSender()] = 0;
        _weiRaised -= weiContributed;
        payable(_msgSender()).transfer(weiContributed);
        _soldTokens = _soldTokens.sub(_getTokenAmount(weiContributed));
    }

    function claim() public ifPresaleEnded ifSoftcapReached{
        address beneficiary = _msgSender();
        uint256 weiAmount = _contributions[beneficiary];
        require(weiAmount > 0, "Wallet has no contribution.");
        uint256 tokenAmount = _getTokenAmount(weiAmount);
        emit TokensPurchased(_msgSender(), beneficiary, weiAmount, tokenAmount);
        _processPurchase(beneficiary, tokenAmount);
        emit Claimed(beneficiary, tokenAmount);
    }

    function _ifEligible(address beneficiary,uint256 weiAmount) internal view {
        weiAmount += _contributions[beneficiary];
        uint256 tokenAmount = _getTokenAmount(weiAmount);
        tokenAmount += _token.balanceOf(beneficiary);
        require(tokenAmount >= _minBuy, "Can not be less than minimum buy.");
        this;
    }
   
}
